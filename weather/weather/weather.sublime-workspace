Mw) index += 1;\n                    if (Rh > Mh) index += 2;\n                    switch (index) {\n                        case 1:\n                            _Rw = Mw;\n                            _Rw = Rh * Mw / Rw;\n                        case 2:\n                            _Rh = Mh;\n                            _Rw = Rw * Mh / Rh;\n                        case 3:\n                            _Rh = (Rh / Mh > Rw / Mw) ? Mh : Rh * Mw / Rw;\n                            _Rw = (Rh / Mh > Rw / Mw) ? Rw * Mh / Rh : Mw\n                    }\n                    if (index != 0) {\n                        need_resize = true\n                    }\n                    return [_Rw, _Rh]\n                }\n                var wh = getRightWH(_image.width, _image.height, _Mw, _Mh);\n                o.style.width = wh[0] + 'px';\n                o.style.height = wh[1] + 'px';\n                o.setAttribute(\"width\", wh[0]);\n                o.setAttribute(\"height\", wh[1]);\n                o.style.visibility = 'visible';\n                if (need_margin == true) {\n                    o.style.marginTop = (Mh - parseInt(wh[1])) / 2 + 'px'\n                }\n                _image = null;\n                return need_resize\n            } /*签名档加载失败处理*/\n            function sign_change_img(img) {\n                img.src = 'http://tb1.bdstatic.com/tb/static-itieba/img/sign_err.png';\n                img.width = 100;\n                img.height = 25;\n            }\n            </script>\n            <div id=\"head\" class=\" search_bright clearfix\" style=\"\">\n                <div class=\"search_top clearfix\">\n                    <a title=\"到贴吧首页\" href=\"/\" class=\"search_logo\" style=\"\"></a>\n                    <div class=\"search_nav j_search_nav\" style=\"\"><a param=\"word\" href=\"http://news.baidu.com/ns?cl=2&amp;rn=20&amp;tn=news&amp;\">新闻</a><a param=\"wd\" href=http://www.baidu.com/s?cl=3&amp;>网页</a><b>贴吧</b><a param=\"word\" href=\"http://zhidao.ba